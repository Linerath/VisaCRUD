@model IEnumerable<Country>

@{
    Layout = null;
}

<div class="sidebar">
    <h4 class="text-center m-1">Страны</h4>
    <div class="countries">
        @foreach (var item in Model)
        {
            @Html.ActionLink(item.Name, "Info", "Visa", new { countryId = item.Id }, new { @class = "country" })
        }
    </div>
</div>
